<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://maven.apache.org/DECORATION/1.6.0"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.6.0 http://maven.apache.org/xsd/decoration-1.6.0.xsd">
    <skin>
        <groupId>lt.velykis.maven.skins</groupId>
        <artifactId>reflow-maven-skin</artifactId>
        <version>1.1.1</version>
    </skin>

    <!--
    <poweredBy>
        <logo name="Karaf" href="http://karaf.apache.org/"
              img="http://karaf.apache.org/images/karaf-logo.png"/>
    </poweredBy>
    -->

    <!--
    <googleAnalyticsAccountId>UA-70313240-1</googleAnalyticsAccountId>
    -->

    <custom>
        <reflowSkin>
            <!-- Make this to 'false' for local development, i.e. file:// URLs -->
            <protocolRelativeURLs>false</protocolRelativeURLs>
            <smoothScroll>true</smoothScroll>
            <theme>default</theme>
            <highlightJs>true</highlightJs>
            <highlightJsTheme>github</highlightJsTheme>
            <absoluteResourceURL>http://beam.incubator.apache.org/</absoluteResourceURL>
            <brand>
                <name><![CDATA[
                    <img src="http://beam.incubator.apache.org/images/beam-123x43.png" />
                ]]></name>
                <href>http://beam.incubator.apache.org</href>
            </brand>
            <!--slogan>Reference implementation of the OASIS Context Server specification</slogan-->
            <titleTemplate>%2$s | %1$s</titleTemplate>
            <toc>sidebar</toc>
            <!--tocTopMax>6</tocTopMax-->
            <topNav>Documentation|Standard|Contribute|Privacy|Community</topNav>
            <bottomNav>
                <column>Documentation|Standard|Contribute|Privacy|Community</column>
                <column>modules</column>
                <column>reports</column>
            </bottomNav>
            <bottomDescription>
                Â© 2016 Apache Software Foundation 
            </bottomDescription>
            <pages>
                <index project="beam-root">
                    <shortTitle>Welcome</shortTitle>
                    <sections>
                        <body />
                        <sidebar />
                    </sections>
                </index>
                <developer-info>
                    <toc>sidebar</toc>
                </developer-info>
                <source-repository>
                    <highlightJs>false</highlightJs>
                </source-repository>
                <issue-tracking>
                    <highlightJs>false</highlightJs>
                    <toc>false</toc>
                </issue-tracking>
                <license>
                    <highlightJs>false</highlightJs>
                    <toc>false</toc>
                </license>
                <!-- Disable ToC for some Maven reports -->
                <project-info>
                    <toc>false</toc>
                </project-info>
                <github-report>
                    <toc>false</toc>
                </github-report>
                <dependencies>
                    <tocTopMax>4</tocTopMax>
                </dependencies>
            </pages>
        </reflowSkin>
    </custom>

    <body>

        <menu name="Documentation" inherit="top">
            <item name="Home" href="./" />
            <item name="Building" href="building.html" />
            <item name="Getting started" href="getting-started.html"/>
            <item name="Guides" href="guides.html" />
            <item name="API and References" href="api.html" />
            <item name="Concepts" href="concepts.html"/>
            <item name="Samples" href="samples.html"/>
        </menu>

        <menu name="parent" ref="parent"/>
        <menu ref="modules"/>
        <menu ref="reports"/>

        <menu name="Privacy" inherit="bottom">
            <item name="Site policy" href="privacy-policy.html"/>
        </menu>

        <menu name="Contribute" inherit="bottom">
            <item name="Mailing lists" href="mail-lists.html"/>
            <item name="Source" href="source-repository.html"/>
            <item name="Issues" href="issue-tracking.html"/>
        </menu>

        <menu name="Community" inherit="top">
            <item name="Mailing lists" href="mail-lists.html"/>
            <item name="Team" href="team-list.html"/>
            <item name="Maturity model report card" href="maturity-model-report.html"/>
        </menu>

    </body>

</project>
